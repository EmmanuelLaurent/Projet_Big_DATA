   "source": [
    "### Regroupement des variables"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "2f8586f9",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Regroupement de la variable âge\n",
    "# On s'aide des Quartile et de la médiane pour les déterminer \n",
    "df_3.loc[df['age'] <= 28,'age'] = '17-28 ans'\n",
    "df_3.loc[(df['age'] >= 29) & (df['age'] <= 37),'age'] = '29-37 ans'\n",
    "df_3.loc[(df['age'] >= 38) & (df['age'] <= 48),'age'] = '38-48 ans'\n",
    "df_3.loc[(df['age'] >= 49) & (df['age'] <= 60),'age'] = '49-60 ans'\n",
    "df_3.loc[df['age'] >= 61,'age'] = '61 ans et +'"
   ]
  },
